    "packages/*",
    "apps/*",
    "libs/*"
  ],
  "dependencies": {
    "lodash": "^4.17.21",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.19"
  },
  "resolutions": {
